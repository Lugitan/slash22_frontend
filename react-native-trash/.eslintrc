{
    "env": {
        "browser": true,
        "es2021": true
    },
    "root" : true,
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react-native"
    ],
    "rules": {
        "no-unused-vars": "warn"
    }
}
